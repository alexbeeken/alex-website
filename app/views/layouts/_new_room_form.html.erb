<%= form_with(model: @room, remote: true, class: "d-flex" ) do |f| %>
  <%= f.text_field :name, class: "form-control", autocomplete: 'off' %>
  <%= f.submit data: { disable_with: false }, class: "bg-custom-one hover:bg-custom-two text-white font-bold py-2 px-4 border border-custom-one rounded cursor-pointer" %>
<% end %>
